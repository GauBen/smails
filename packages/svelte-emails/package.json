    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.21",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
